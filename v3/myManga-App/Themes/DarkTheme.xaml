<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Brush x:Key="window-background">#FF1E1E1E</Brush>
    <Color x:Key="window-background-color">#FF1E1E1E</Color>
    <Brush x:Key="window-foreground">#FFEFEFF2</Brush>
    <Color x:Key="window-foreground-color">#FFEFEFF2</Color>
    <Brush x:Key="window-foreground-disabled">#FF989898</Brush>
    <Color x:Key="window-foreground-disabled-color">#FF989898</Color>

    <Brush x:Key="window-theme-primary">#FF80326E</Brush>
    <Color x:Key="window-theme-primary-color">#FF80326E</Color>
    <Brush x:Key="window-theme-primary-disabled">#FF919191</Brush>
    <Color x:Key="window-theme-primary-disabled-color">#FF919191</Color>

    <Brush x:Key="window-theme-secondary">#FFD3D3D3</Brush>
    <Color x:Key="window-theme-secondary-color">#FFD3D3D3</Color>
    <Brush x:Key="window-theme-secondary-disabled">#FFEFEFF2</Brush>
    <Color x:Key="window-theme-secondary-disabled-color">#FFEFEFF2</Color>

    <Brush x:Key="text-dark">#FFFFFF</Brush>
    <Brush x:Key="text-muted">#666666</Brush>
    <Brush x:Key="text-light">#333333</Brush>
</ResourceDictionary>