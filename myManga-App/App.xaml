<Application x:Class="myManga_App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- DO NOT MOVE THIS! DO NOT ADD ANYTHING BEFORE THIS! -->
                <ResourceDictionary x:Name="ThemeResourceDictionary" Source="/myManga;component/Themes/LightTheme.xaml"/>

                <!-- DO NOT MOVE THIS! DO NOT ADD ANYTHING BEFORE THIS! -->
                <ResourceDictionary x:Name="LocalizationResourceDictionary" Source="/myManga;component/Resources/Localization/Dictionary_en.xaml"/>

                <ResourceDictionary Source="/myManga;component/Themes/CoreResourceDictionary.xaml"/>
                <ResourceDictionary Source="/myManga;component/Controls/Window/myMangaWindow.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
